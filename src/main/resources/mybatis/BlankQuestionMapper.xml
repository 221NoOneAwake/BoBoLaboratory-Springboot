<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.bobolaboratory.springboot.mapper.BlankQuestionMapper">
    <insert id="addBlankQuestion" parameterType="cn.bobolaboratory.springboot.entity.BlankQuestion">
        insert into bobo.blankquestion (questionid, question, answer)
            VALUE (#{questionid}, #{question}, #{answer})
    </insert>

    <select id="queryAllBlankQuestion" resultType="cn.bobolaboratory.springboot.entity.BlankQuestion">
        select *
        from bobo.blankquestion;
    </select>
</mapper>